<mapper namespace="com.itda.ITDA.mybatis.mapper.ChannelMapper">
    
    <select id="getChannelDetail" resultType="ChannelList">
        SELECT *
        FROM CHANNELLIST
        WHERE CHNUM = #{chnum}
    </select>
    
 
